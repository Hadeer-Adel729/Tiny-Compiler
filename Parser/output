    [Read][x]
    [If]
     [Oper][LESS_THAN]
         [NUM][0]
         [ID][x]
       [Assign][fact]
            [NUM][1]
       [Repeat]
          [Assign][fact]
             [Oper][TIMES]
               [ID][fact]
               [ID][x]
          [Assign][x]
            [Oper][MINUS]
               [ID][x]
               [NUM][1]
        [Oper][EQUAL]
            [ID][x]
            [NUM][0]
       [Write]
            [ID][fact]
